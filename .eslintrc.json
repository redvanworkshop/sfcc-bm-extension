{
    "env": {
        "browser": true,
        "es2021": true
    },
    "globals": {
      "browser": true,
      "chrome": true,
      "io": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "plugins": [
      "html"
    ],
    "rules": {
      "quotes": [2, "single", { "allowTemplateLiterals": true }],
      "arrow-parens": 0,
      "no-prototype-builtins": 0,
      "generator-star-spacing": 0
    }
}
